<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Start5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView13"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/start5_bar"
            android:textSize="16sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="100sp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/start5_setumei" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="135dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/m_kajyuaru" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="135dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/m_sinnpuru" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="135dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/s" />
        </LinearLayout>

        <RadioGroup
            android:id="@+id/RadioGroup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButton3"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:text="カジュアル" />

            <RadioButton
                android:id="@+id/radioButton4"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:text="シンプル" />

            <RadioButton
                android:id="@+id/radioButton6"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:text="クール" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </RadioGroup>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="76dp" />


        <TextView
            android:id="@+id/start_mens_error"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#FCE93232" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/back_start5"
                android:layout_width="206dp"
                android:layout_height="wrap_content"
                android:text="@string/back" />

            <Button
                android:id="@+id/next_start_mens"
                android:layout_width="206dp"
                android:layout_height="wrap_content"
                android:text="@string/finish" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>